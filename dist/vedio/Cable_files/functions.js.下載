$(document).ready(function(){	/* Start SFCHANGE1 - ExRx app pre-release email prompt - 11/3/2018 */		$('.virtual-link').on('click', function(e) {		var link = $(this).find('.virtual-link-target');		window.location.href = link.attr('href');	});		var submitFn = function(e,callback) {		e.preventDefault();		var self = $(this);		if(self.hasClass('saving')) {			return;		} else {			self.removeClass('failed');			self.removeClass('saved');			self.removeClass('network-error');			self.addClass('saving');			var url = self.attr('action');			$.post(url,self.serializeArray()).done(callback).fail(function() {				self.removeClass('saving').addClass('network-error');			});		}	};	$('.ajax-form').on('submit',submitFn);	$('.ajax-form .ajax-submit-btn').on('click',function(e) {		submitFn.bind($(this).closest('.ajax-form')[0])(e);	});	$('#close-inline-app-prompt').on('click', function(e) {		$('#inline-app-prompt').remove();		var dismissedCount = window.localStorage.getItem('app_dismiss_count') || 0;		dismissedCount = Number(dismissedCount)+1;		var newDate;		if(dismissedCount >= 2) {			newDate = ((Date.now().valueOf()+24*60*60*1000*9999999));		} else {			newDate = ((Date.now().valueOf()+24*60*60*1000*3));		}		window.localStorage.setItem('show_app_waitlist',newDate);		window.localStorage.setItem('app_dismiss_count',dismissedCount);	});	var links = document.querySelectorAll('.learn-more-link');	var learnMoreLinkFn = function(link) {		link.addEventListener('click',function(e) {			var emailField = $('#app-waitlist-email');			if(emailField.val().length > 0) {				var fn = submitFn.bind($('#app-waitlist-form')[0]);				fn(e,function() {					if($('#app-waitlist-form').hasClass('saved'))						window.location.href = link.href;				});			} else {				window.localStorage.setItem('show_app_waitlist',((Date.now().valueOf()+24*60*60*1000*3)));			}		});	};	var i, len, link;	for (i = 0, len = links.length; i < len; i++) {		link = links[i];		learnMoreLinkFn(link);	}	/* End SFCHANGE1 - ExRx app pre-release email prompt - 11/3/2018 */});//doc.ready